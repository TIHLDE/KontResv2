{
  "importOrder": [
    "^@/types/*",
    "<THIRD_PARTY_MODULES>",
    "^@/server/*",
    "^@/components/*",
    "^@/utils/*",
    "^.*$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "all"
}
